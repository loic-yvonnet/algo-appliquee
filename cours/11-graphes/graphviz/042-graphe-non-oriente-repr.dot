graph G {
    bgcolor="transparent";

    /* Entities */
    A [label="0", shape="circle"];
    B [label="1", shape="circle"];
    C [label="2", shape="circle"];
    D [label="3", shape="circle"];
    E [label="4", shape="circle"];

    /* Relationships */
    A -- B [label="a"];

    B -- D [label="b"];
    B -- E [label="c"];

    C -- E [label="d"];

    D -- A [label="e"];
    D -- C [label="f"];

    /* Ranks */
    { rank=same; };
}