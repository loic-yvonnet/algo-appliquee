{
  "name": "algo-appliquee",
  "version": "0.0.1",
  "author": "Loïc Yvonnet <loic@yvo.solutions>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run -s clean && npm run -s assets && npm run -s all_md_slides && npm run -s other_md",
    "clean": "rimraf dist",
    "assets": "cpy \"assets/**/*\" dist/assets",
    "lecture-00": "marp --no-stdin cours/00-avant-propos/slides.md         -o dist/00-avant-propos/index.html",
    "lecture-01": "marp --no-stdin cours/01-intro-programmation/slides.md  -o dist/01-intro-programmation/index.html",
    "lecture-02": "marp --no-stdin cours/02-bases-python/slides.md         -o dist/02-bases-python/index.html",
    "lecture-03": "marp --no-stdin cours/03-programmes-simples/slides.md   -o dist/03-programmes-simples/index.html",
    "lecture-04": "marp --no-stdin cours/04-fonctions/slides.md            -o dist/04-fonctions/index.html",
    "lecture-05": "marp --no-stdin cours/05-structures-donnees/slides.md   -o dist/05-structures-donnees/index.html",
    "lecture-06": "marp --no-stdin cours/06-problemes-classiques/slides.md -o dist/06-problemes-classiques/index.html",
    "lecture-07": "marp --no-stdin cours/07-complexite/slides.md           -o dist/07-complexite/index.html",
    "lecture-08": "marp --no-stdin cours/08-tests/slides.md                -o dist/08-tests/index.html",
    "lecture-09": "marp --no-stdin cours/09-recherche-et-tri/slides.md     -o dist/09-recherche-et-tri/index.html",
    "lecture-10": "marp --no-stdin cours/10-python-avance/slides.md        -o dist/10-python-avance/index.html",
    "lecture-11": "marp --no-stdin cours/11-graphes/slides.md              -o dist/11-graphes/index.html",
    "lecture-12": "marp --no-stdin cours/12-conclusion/slides.md           -o dist/12-conclusion/index.html",
    "all_md_slides": "npm run -s lecture-00 && npm run -s lecture-01 && npm run -s lecture-02 && npm run -s lecture-03 && npm run -s lecture-04 && npm run -s lecture-05 && npm run -s lecture-06 && npm run -s lecture-07 && npm run -s lecture-08 && npm run -s lecture-09 && npm run -s lecture-10 && npm run -s lecture-11 && npm run -s lecture-12",
    "other_md": "eleventy"
  },
  "keywords": [
    "Algorithmique",
    "Appliquée",
    "BTS",
    "SIO",
    "SISR",
    "Python"
  ],
  "devDependencies": {
    "@marp-team/marp-cli": "^1.1.1",
    "@11ty/eleventy": "^0.12.1",
    "cpy-cli": "^3.1.1",
    "rimraf": "^3.0.2",
    "html-minifier": "^4.0.0"
  }
}
