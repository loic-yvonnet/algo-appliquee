{
  "name": "algo-appliquee",
  "version": "0.0.1",
  "author": "Loïc Yvonnet <loic@yvo.solutions>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build_site": "eleventy",
    "build_all_but_pdf": "npm run -s clean && npm run -s build_site && npm run -s all_md_slides",
    "prepare_dev_env": "cpy includes/layouts/root.dev.njk  includes/layouts --rename=root.njk",
    "prepare_prod_env": "cpy includes/layouts/root.prod.njk includes/layouts --rename=root.njk",
    "build_local": "npm run -s prepare_dev_env && npm run -s build_all_but_pdf",
    "build": "npm run -s prepare_prod_env && npm run -s build_all_but_pdf && npm run -s all_md_pdfs",
    "clean": "rimraf dist",
    "watch": "eleventy --serve",
    "lecture-00": "marp --no-stdin cours/00-avant-propos/slides.md         -o dist/00-avant-propos/diaporama.html",
    "lecture-01": "marp --no-stdin cours/01-intro-programmation/slides.md  -o dist/01-intro-programmation/diaporama.html",
    "lecture-02": "marp --no-stdin cours/02-bases-python/slides.md         -o dist/02-bases-python/diaporama.html",
    "lecture-03": "marp --no-stdin cours/03-programmes-simples/slides.md   -o dist/03-programmes-simples/diaporama.html",
    "lecture-04": "marp --no-stdin cours/04-fonctions/slides.md            -o dist/04-fonctions/diaporama.html",
    "lecture-05": "marp --no-stdin cours/05-structures-donnees/slides.md   -o dist/05-structures-donnees/diaporama.html",
    "lecture-06": "marp --no-stdin cours/06-problemes-classiques/slides.md -o dist/06-problemes-classiques/diaporama.html",
    "lecture-07": "marp --no-stdin cours/07-complexite/slides.md           -o dist/07-complexite/diaporama.html",
    "lecture-08": "marp --no-stdin cours/08-tests/slides.md                -o dist/08-tests/diaporama.html",
    "lecture-09": "marp --no-stdin cours/09-recherche-et-tri/slides.md     -o dist/09-recherche-et-tri/diaporama.html",
    "lecture-10": "marp --no-stdin cours/10-python-avance/slides.md        -o dist/10-python-avance/diaporama.html",
    "lecture-11": "marp --no-stdin cours/11-graphes/slides.md              -o dist/11-graphes/diaporama.html",
    "lecture-12": "marp --no-stdin cours/12-conclusion/slides.md           -o dist/12-conclusion/diaporama.html",
    "all_md_slides": "npm run -s lecture-00 && npm run -s lecture-01 && npm run -s lecture-02 && npm run -s lecture-03 && npm run -s lecture-04 && npm run -s lecture-05 && npm run -s lecture-06 && npm run -s lecture-07 && npm run -s lecture-08 && npm run -s lecture-09 && npm run -s lecture-10 && npm run -s lecture-11 && npm run -s lecture-12",
    "pdf-00": "marp --no-stdin cours/00-avant-propos/slides.md         -o dist/00-avant-propos/diaporama.pdf --pdf",
    "pdf-01": "marp --no-stdin cours/01-intro-programmation/slides.md  -o dist/01-intro-programmation/diaporama.pdf --pdf",
    "pdf-02": "marp --no-stdin cours/02-bases-python/slides.md         -o dist/02-bases-python/diaporama.pdf --pdf",
    "pdf-03": "marp --no-stdin cours/03-programmes-simples/slides.md   -o dist/03-programmes-simples/diaporama.pdf --pdf",
    "pdf-04": "marp --no-stdin cours/04-fonctions/slides.md            -o dist/04-fonctions/diaporama.pdf --pdf",
    "pdf-05": "marp --no-stdin cours/05-structures-donnees/slides.md   -o dist/05-structures-donnees/diaporama.pdf --pdf",
    "pdf-06": "marp --no-stdin cours/06-problemes-classiques/slides.md -o dist/06-problemes-classiques/diaporama.pdf --pdf",
    "pdf-07": "marp --no-stdin cours/07-complexite/slides.md           -o dist/07-complexite/diaporama.pdf --pdf",
    "pdf-08": "marp --no-stdin cours/08-tests/slides.md                -o dist/08-tests/diaporama.pdf --pdf",
    "pdf-09": "marp --no-stdin cours/09-recherche-et-tri/slides.md     -o dist/09-recherche-et-tri/diaporama.pdf --pdf",
    "pdf-10": "marp --no-stdin cours/10-python-avance/slides.md        -o dist/10-python-avance/diaporama.pdf --pdf",
    "pdf-11": "marp --no-stdin cours/11-graphes/slides.md              -o dist/11-graphes/diaporama.pdf --pdf",
    "pdf-12": "marp --no-stdin cours/12-conclusion/slides.md           -o dist/12-conclusion/diaporama.pdf --pdf",
    "all_md_pdfs": "npm run -s pdf-00 && npm run -s pdf-01 && npm run -s pdf-02 && npm run -s pdf-03 && npm run -s pdf-04 && npm run -s pdf-05 && npm run -s pdf-06 && npm run -s pdf-07 && npm run -s pdf-08 && npm run -s pdf-09 && npm run -s pdf-10 && npm run -s pdf-11 && npm run -s pdf-12"
  },
  "keywords": [
    "Algorithmique",
    "Appliquée",
    "BTS",
    "SIO",
    "SISR",
    "Python"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@marp-team/marp-cli": "^1.3.0",
    "cpy-cli": "^3.1.1",
    "html-minifier": "^4.0.0",
    "katex": "^0.13.13",
    "markdown-it-katex": "^2.0.3",
    "rimraf": "^3.0.2"
  }
}
